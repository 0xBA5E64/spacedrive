{
  "pluginSearchDirs": [
    "."
  ],
  "useTabs": true,
  "printWidth": 100,
  "singleQuote": true,
  "trailingComma": "none",
  "bracketSameLine": false,
  "semi": true,
  "quoteProps": "consistent",
  "importOrder": [
    "^[./]",
    "^@sd/ui/(.*)$",
    "^@sd/client/(.*)$",
    "^@sd/interface/(.*)$"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true
}
